<?xml version="1.0" encoding="utf-8"?>
<rotate xmlns:android="http://schemas.android.com/apk/res/android"
    android:fromDegrees="-90"
    android:toDegrees="270"
    android:pivotX="50%"
    android:pivotY="50%">
    <shape
        android:shape="ring"
        android:useLevel="false">
        <gradient
            android:centerColor="#daf4e6"
            android:endColor="#4be895"
            android:centerY="0.5"
            android:type="sweep"
            android:useLevel="false"
            />
    </shape>

</rotate>